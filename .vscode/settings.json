{
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "deno.lint": true,
  "deno.unstable": true,
  "editor.formatOnSave": true,
  "editor.quickSuggestions": {
    "strings": true
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "files.exclude": {
    "**/*tsconfig.json": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.retool_types/**": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/Thumbs.db": true,
    ".cache": true,
    "retool.config.json": true
  },
  "i18n-ally.localesPaths": [],
  "typescript.tsdk": "node_modules/typescript/lib"
}
