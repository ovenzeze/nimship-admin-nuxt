{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "target": "ES2020",
        "jsx": "react",
        "allowImportingTsExtensions": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "types": [
            "@nuxt/types",
            "@nuxt/content",
            "@nuxtjs/i18n"
        ]
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ],
    "extends": "./.nuxt/tsconfig.json",
}